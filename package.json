{
  "name": "contexto-mcp",
  "version": "0.1.5",
  "description": "An AI-powered documentation server that uses the Model Context Protocol (MCP) to make your project's documentation available to AI assistants.",
  "main": "build/src/index.js",
  "type": "module",
  "bin": {
    "contexto": "build/src/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Danielratmiroff/mcp-docs.git"
  },
  "scripts": {
    "build": "npx tsc",
    "start": "node --loader ts-node/esm ./src/index.ts",
    "inspect": "pnpm build && npx @modelcontextprotocol/inspector node --loader ts-node/esm ./src/index.ts",
    "test": "jest",
    "lint": "pnpm eslint",
    "check": "pnpm test",
    "prepare": "pnpm build",
    "pre-commit": "pnpm check && pnpm build",
    "prepublishOnly": "pnpm build"
  },
  "keywords": [
    "mcp",
    "ai",
    "documentation"
  ],
  "author": "Daniel Ratmiroff",
  "license": "Apache-2.0",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.13.3",
    "@xenova/transformers": "^2.17.2",
    "ml-distance": "^4.0.1",
    "prettier": "^3.6.2",
    "sharp": "^0.34.3",
    "tailwindcss": ">=3.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@modelcontextprotocol/inspector": "^0.15.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.19.4",
    "cheminfo-types": "^1.8.1",
    "eslint": "^9.30.1",
    "globals": "^16.3.0",
    "jest": "^30.0.4",
    "ts-jest": "^29.4.0",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.35.1"
  }
}
